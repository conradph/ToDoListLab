@model Employee

<h2>@Model.Name</h2>
<br />
<br />
<p>Hours: @Model.Hours</p>
<p>Title: @Model.Title</p>

<p>Are you sure you wish to delete this employee?</p>
<a asp-action="DeleteFromDb" asp-controller="Employee" asp-route-id="@Model.Id"><button>Yes</button></a>
<a asp-action="Index" asp-controller="Employee"><button>No</button></a>
